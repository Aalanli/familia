interface Foo {
    fn say_hello(this)
}

class Bar for Foo({a: ()}) {
    fn say_hello(this) {
        print("Hello, world!\n");
    }
}

class Baz for Foo({a: ()}) {
    fn say_hello(this) {
        print("Hello again.\n");
    }
}


fn foo(a: Foo) {
    a.say_hello();
}

fn main() {
    let b = Bar({a: ()});
    foo(b);
    let c = Baz({a: ()});
    foo(c);
}
